              char PWM_SetDUTY (uchar duty)
              {
                      PWM_Stop ();
                      
                      PWM_OUT    = 1;
                      PWM_CURPOS = 0;
                      
                      if (duty == 0) {
                              PWM_PosCYC = 50;
                              PWM_NegCYC = 50;
                      } else if (duty > 100){
                              PWM_PosCYC = 50;
                              PWM_NegCYC = 50;
                      } else {
