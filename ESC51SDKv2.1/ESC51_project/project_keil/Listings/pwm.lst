              char PWM_Init (uchar duty)
              {
                      if (duty == 0) {
                              PWM_PosCYC = 50;
                              PWM_NegCYC = 50;
                      } else if (duty > 100){
                              PWM_PosCYC = 50;
                              PWM_NegCYC = 50;
                      } else {
                              PWM_PosCYC = duty;
                              PWM_NegCYC = 100 - PWM_PosCYC;
                      }
                      
                      PWM_OUT    = 1;
                      PWM_CURPOS = 0;
                      
                      GTimer_OBJJoin (pwm_int_function, NULL);
                      
                      return 0;
              }
              
              
              char PWM_Start (void)
              {
              
