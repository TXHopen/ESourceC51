              void UART_SendByte(u8 dat)
              {
                      ES = 0;               //关闭串口中断
                      TI = 0;               //清发送完毕中断请求标志位
                      SBUF = dat;           //发送
                      while(TI == 0);       //等待发送完毕
                      TI = 0;               //清发送完毕中断请求标志位
                      ES = 1;               //允许串口中断
              }
              
